{
  "name": "ng-packaged",
  "version": "0.0.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "ng build --prod --base-href \"/ngx-mock-server/\"",
    "build:app": "cpr dist/my-lib src/@my/lib && ng build packages && rimraf src/@my",
    "build:lib": "rimraf dist && ng-packagr -p mock-server/package.json",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "ngh": "angular-cli-ghpages --repo=https://GH_TOKEN@github.com/dima11221122/ngx-mock-server --dir dist/ngx-mock-server",
    "publish": "npm publish mock-server/dist"
  },
  "dependencies": {
    "@angular/animations": "^6.0.5",
    "@angular/cdk": "^6.2.1",
    "@angular/common": "6.0.3",
    "@angular/compiler": "6.0.3",
    "@angular/core": "6.0.3",
    "@angular/forms": "6.0.3",
    "@angular/http": "6.0.3",
    "@angular/material": "^6.2.1",
    "@angular/platform-browser": "6.0.3",
    "@angular/platform-browser-dynamic": "6.0.3",
    "@angular/router": "6.0.3",
    "core-js": "^2.4.1",
    "rxjs": "^6.0.0-beta.1",
    "zone.js": "0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.6.0",
    "@angular-devkit/schematics": "^0.6.0",
    "@angular/cli": "6.0.5",
    "@angular/compiler-cli": "6.0.3",
    "@angular/language-service": "6.0.3",
    "@types/jasmine": "~2.8.0",
    "@types/node": "~10.1.0",
    "codelyzer": "~4.3.0",
    "cpr": "^3.0.0",
    "jasmine-core": "~3.1.0",
    "jasmine-spec-reporter": "~4.2.0",
    "karma": "~2.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^2.0.0",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^1.0.0",
    "ng-packagr": "3.0.0",
    "protractor": "~5.3.0",
    "rimraf": "^2.6.1",
    "ts-node": "~6.0.0",
    "tsickle": "^0.29.0",
    "tslib": "^1.7.1",
    "tslint": "~5.10.0",
    "typescript": "~2.7.2",
    "angular-cli-ghpages": "^0.5.2"
  }
}
